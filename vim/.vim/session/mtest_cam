silent only
cd ~/tools/testing/mtest4/tests/cameras
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +619 ~/tools/testing/mtest4/tests/cameras/ecam.py
