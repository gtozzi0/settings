set splitbelow splitright
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
wincmd t
set winheight=1 winwidth=1
exe 'vert 1resize ' . ((&columns * 116 + 106) / 212)
exe 'vert 2resize ' . ((&columns * 95 + 106) / 212)
argglobal
setlocal fdm=manual
